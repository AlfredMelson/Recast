{
  "name": "recast",
  "version": "1.0.0",
  "private": true,
  "description": "A web-based tool to recast, type, and visualize JSON",
  "tags": [
    "json",
    "editor",
    "recast",
    "viewer",
    "format",
    "typescript"
  ],
  "keywords": [
    "mui",
    "react",
    "recoil",
    "monaco",
    "typescript",
    "xstate",
    "javascript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AlfredMelson/recast.git"
  },
  "author": "Alfred Melson (@vmelson)",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@monaco-editor/react": "^4.3.1",
    "@mui/icons-material": "^5.2.5",
    "@mui/lab": "5.0.0-alpha.63",
    "@mui/material": "^5.2.7",
    "@primer/primitives": "^7.4.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^0.24.0",
    "file-saver": "^2.0.5",
    "framer-motion": "5.5.5",
    "lodash": "^4.17.21",
    "react": "^18.0.0-rc.0-next-fe419346d-20220105",
    "react-dom": "^18.0.0-rc.0-next-fe419346d-20220105",
    "react-intersection-observer": "^8.33.1",
    "react-router-dom": "6.2.1",
    "react-scripts": "^5.0.0",
    "react-spring": "^9.4.1",
    "react-swipeable-views": "^0.14.0",
    "recoil": "^0.5.2",
    "sass": "^1.46.0"
  },
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build": "react-scripts build",
    "start": "react-scripts start",
    "eject": "react-scripts eject",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|ts|tsx|md|mdx|gql|graphql)\"",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "typecheck": "tsc --pretty",
    "fulltest": "yarn format && yarn lint && yarn typecheck",
    "unimported": "npx unimported",
    "clean": "lerna clean --yes && rimraf node_modules yarn.lock"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "node_modules/variables/.+\\.(j|t)sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!variables/.*)"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "devDependencies": {
    "@testing-library/react-hooks": "^7.0.2",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/file-saver": "^2.0.4",
    "@types/jest": "^27.4.0",
    "@types/jsoneditor": "^9.5.1",
    "@types/lodash": "^4.14.178",
    "@types/node": "17.0.8",
    "@types/prettier": "^2.4.2",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@types/react-router-dom": "5.3.2",
    "@types/react-swipeable-views": "^0.13.1",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^27.4.7",
    "lerna": "^4.0.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
